{
  "name": "HasanHawary/report-generator",
  "description": "A modular Laravel report generator for cards, charts, and tables.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "report",
    "reports",
    "charts",
    "tables",
    "analytics",
    "dashboard"
  ],
  "authors": [
    {
      "name": "Hassan",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^10.0 || ^11.0 || ^12.0",
    "laravel/framework": "^10.0 || ^11.0 || ^12.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0 || ^9.0 || ^10.0",
    "phpunit/phpunit": "^9.6 || ^10.5 || ^11.0",
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "HasanHawary\\ReportGenerator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "HasanHawary\\ReportGenerator\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "format": "php-cs-fixer fix --allow-risky=yes",
    "check-style": "php-cs-fixer fix --dry-run --diff"
  },
  "extra": {
    "laravel": {
      "providers": [
        "HasanHawary\\ReportGenerator\\ReportGeneratorServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
